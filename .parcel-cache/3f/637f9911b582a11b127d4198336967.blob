.listContainer {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  box-sizing: border-box;
  overflow-x: auto;
}

.airport {
  background: #fff;
  padding: 20px;
  margin: 0 5px;
  display: flex;
  height: min-content;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}

.image {
  width: 60px;
}