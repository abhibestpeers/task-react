.listContainer {
  width: 30%;
  height: 40%;
  display: block;
  box-sizing: border-box;
  overflow-x: auto;
  border: 2px solid red;
  margin-top: 20px;
  overflow: hidden;
}

.airport {
  display: flex;
  background: #fff;
  height: min-content;
  width: 100%;
  box-sizing: border-box;
  align-items: flex-start;
}

.image {
  width: 100%;
  height: 100%;
}

p {
  position: relative;
  top: 0;
  color: white;
}