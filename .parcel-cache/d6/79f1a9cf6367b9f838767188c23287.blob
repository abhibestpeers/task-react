.listContainer {
  width: 30%;
  height: auto;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
  overflow-x: auto;
  border: 2px solid red;
  margin-top: 20px;
}

.airport {
  background: #fff;
  display: flex;
  width: 100%;
  box-sizing: border-box;
}

.image {
  width: 100%;
}

p {
  position: relative;
  top: 100%;
  right: 50%;
}