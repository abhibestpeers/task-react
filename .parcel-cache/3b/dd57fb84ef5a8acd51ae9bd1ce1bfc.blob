.listContainer {
  width: 30%;
  height: auto;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
  overflow-x: auto;
  border: 2px solid red;
  margin-top: 20px;
}

.airport {
  background: #fff;
  padding: 20px;
  display: flex;
  width: 100%;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}

.image {
  width: 100%;
}